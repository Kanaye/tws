{
  "name": "tws",
  "version": "0.0.1",
  "description": "A low-level library to connect to the twitch chat.",
  "main": "dist/node.js",
  "browser": "dist/index.mjs",
  "typings": "dist/typeings/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "tslint": "tslint -p . -c tslint.json",
    "tslint:fix": "tslint -p . --fix -c tslint.json",
    "prettier": "prettier --write ./{src,test}/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s tslint:fix prettier build test"
    }
  },
  "repository": {
    "type": "git",
    "url": "github.com/kanaye/tws.git"
  },
  "keywords": [
    "Chat",
    "Twitch"
  ],
  "author": "Joscha Rohmann <joscha@kanaye.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@microsoft/api-extractor": "^5.10.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.6",
    "dset": "^1.0.1",
    "fast-async": "^7.0.6",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.5.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^4.0.0-beta.8",
    "rollup-plugin-typescript2": "^0.16.1",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1",
    "ws": "^6.0.0"
  },
  "build": {
    "umdName": "Tws"
  }
}

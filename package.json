{
  "name": "@jancba/tws",
  "version": "0.0.2",
  "description": "A low-level library to connect to the twitch chat.",
  "main": "dist/node.js",
  "browser": "dist/index.mjs",
  "typings": "dist/typeings/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "tslint": "tslint -p . -c tslint.json",
    "tslint:fix": "tslint -p . --fix -c tslint.json",
    "prettier": "prettier --write ./{src,test}/**/*.ts",
    "lint-all": "run-s prettier tslint:fix",
    "add-staged": "git add .",
    "ci": "run-s tslint build test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint-all build test add-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "github.com/jancba/tws.git"
  },
  "keywords": [
    "Chat",
    "Twitch"
  ],
  "author": "Joscha Rohmann <joscha@kanaye.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/jest": "^24.0.5",
    "@types/node": "^13.1.0",
    "dset": "^2.0.0",
    "fast-async": "^7.0.6",
    "husky": "^3.0.4",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rollup": "^2.9.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-typescript2": "^0.25.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.1.3",
    "ws": "^7.0.0"
  },
  "build": {
    "umdName": "Tws"
  }
}
